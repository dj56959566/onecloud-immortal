# seed.config - minimal deterministic .config for automated build
# Target: arm (ARMv7, OneCloud) - adjust CONFIG_TARGET_DEVICE_* to match actual profile if known.

CONFIG_TARGET_arm=y
CONFIG_TARGET_arm_cortex-a7=y
CONFIG_TARGET_immortalwrt=y

# Kernel
CONFIG_KERNEL_PATCHVER="6.6"

# Base
CONFIG_BUILD_PATENTED=n
CONFIG_HAVE_DOT_CONFIG=y

# LuCI (web)
CONFIG_PACKAGE_luci=y
CONFIG_PACKAGE_luci-ssl-openssl=y
CONFIG_PACKAGE_luci-theme-bootstrap=y

# Essential utilities
CONFIG_PACKAGE_dropbear=y
CONFIG_PACKAGE_opkg=y
CONFIG_PACKAGE_urngd=y
CONFIG_PACKAGE_block-mount=y
CONFIG_PACKAGE_kmod-usb-storage=y
CONFIG_PACKAGE_kmod-fs-ext4=y

# Selected apps
CONFIG_PACKAGE_luci-app-openclash=y
CONFIG_PACKAGE_nikki=y
CONFIG_PACKAGE_dockerd=y
CONFIG_PACKAGE_luci-app-dockerman=y
CONFIG_PACKAGE_docker-compose=y
CONFIG_PACKAGE_netdata=y
CONFIG_PACKAGE_luci-app-netdata=y
CONFIG_PACKAGE_luci-app-filetransfer=y
CONFIG_PACKAGE_ttyd=y
CONFIG_PACKAGE_luci-app-ttyd=y
CONFIG_PACKAGE_cronie=y
